    "pino": "^9.0.0",
    "pinorama-client": "workspace:*",
    "pinorama-server": "workspace:*",
    "pinorama-typescript-config": "workspace:*",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  },
  "publishConfig": {
